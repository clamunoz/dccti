  <rdf:Description rdf:about="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#plan2015">
    <rdf:type rdf:resource="http://parra.chillan.ubiobio.cl:8070/clamunoz/bmm#BusinessPlan"/>
    <dc11:title>PE DCCTI 2015-2019</dc11:title>
    <ns0:CreationDate>30-10-2017</ns0:CreationDate>
    <ns0:Creator>DCCTI</ns0:Creator>
    <dc11:defines rdf:resource="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#mission"/>
    <dc11:defines rdf:resource="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#vision"/>
    <dc11:defines rdf:resource="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#strategy1"/>
    <dc11:defines>
      <rdf:Description rdf:about="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#tactic1">
        <rdf:type rdf:resource="http://parra.chillan.ubiobio.cl:8070/clamunoz/bmm#Tactic"/>
        <ns1:elementOf rdf:resource="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#plan2015"/>
        <ns1:identifier>tac1</ns1:identifier>
        <ns1:title>Tactic 1</ns1:title>
        <ns1:validity>does not record validity</ns1:validity>
        <ns1:description>Fortalecer los sistemas de medición, seguimiento y control de las fases que comprenden el proceso de armonización de los programas de pregrado con el modelo educativo institucional, actualizando y sintonizando las normativas y reglamentación correspondiente.</ns1:description>
        <ns1:implements rdf:resource="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#strategy1"/>
        <ns1:achieves rdf:resource="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#objective1"/>
        <ns1:achieves>
          <ns1:Objective rdf:about="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#objective2">
            <ns1:elementOf rdf:resource="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#plan2015"/>
            <ns1:identifier>obj2</ns1:identifier>
            <ns1:title>Objective 2</ns1:title>
            <ns1:description>Capacitar a los docentes JC y JP en el proceso de seguimiento y monitoreo de acuerdo a la reglamentación vigente - 100% de los docentes capacitados.</ns1:description>
            <ns1:measure>(N° docentes capacitados / N° docentes JC y JP)*100.</ns1:measure>
            <ns1:timetargeted>04-05-2015 al 31-12-2019</ns1:timetargeted>
            <ns1:quantifies rdf:resource="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#goal1"/>
          </ns1:Objective>
        </ns1:achieves>

        <ns1:achieves>
          <ns1:Objective rdf:about="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#objective3">
            <ns1:elementOf rdf:resource="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#plan2015"/>
            <ns1:identifier>obj3</ns1:identifier>
            <ns1:title>Objective 3</ns1:title>
            <ns1:description>Usar plataformas educativas para la gestión en asignaturas impartidas por el DCCTI - 70% de asignaturas en plataformas educativas</ns1:description>
            <ns1:measure>(N° de asignaturas que usan plataformas educativas / Total de asignaturas impartidas por el DCCTI)*100.</ns1:measure>
            <ns1:timetargeted>04-05-2015 al 31-12-2019</ns1:timetargeted>
            <ns1:quantifies rdf:resource="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#goal1"/>
          </ns1:Objective>
        </ns1:achieves>

      </rdf:Description>
    </dc11:defines>

    <dc11:defines rdf:resource="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#goal1"/>
    <dc11:defines rdf:resource="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#objective1"/>
  </rdf:Description>

  <ns1:Mission rdf:about="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#mission">
    <ns1:elementOf rdf:resource="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#plan2015"/>
    <ns1:identifier>mis1</ns1:identifier>
    <ns1:title>Mission 1</ns1:title>
    <ns1:description>Cultivar las disciplinas de Ciencias de la Computación y Tecnologías de la Información, a fin de contribuir en la formación de profesionales a través de la docencia de pre y postgrado, difundiendo el conocimiento asociado a estas, tanto a la comunidad científica como a la sociedad en su conjunto, impulsando el emprendimiento y la innovación para el desarrollo regional y nacional.</ns1:description>
    <ns1:makesOperative rdf:resource="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#vision"/>
    <ns1:isPlanedByMeansOf rdf:resource="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#strategy1"/>
  </ns1:Mission>

  <ns1:Vision rdf:about="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#vision">
    <ns1:elementOf rdf:resource="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#plan2015"/>
    <ns1:identifier>vis1</ns1:identifier>
    <ns1:title>Vision 1</ns1:title>
    <ns1:description>Ser reconocido por su aporte académico y científico en el área de Ciencias de la Computación y  Tecnologías de Información a nivel regional, nacional e internacional; con un cuerpo académico de excelencia, vinculado con el medio, integrado a redes científicas y académicas, que contribuya a la formación de personas integrales y socialmente responsables.</ns1:description>
  </ns1:Vision>

  <ns1:Strategy rdf:about="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#strategy1">
    <ns1:elementOf rdf:resource="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#plan2015"/>
    <ns1:identifier>str1</ns1:identifier>
    <ns1:title>Strategy 1</ns1:title>
    <ns1:description>DOCENCIA DE PREGRADO FORMANDO PERSONAS INTEGRALES DE EXCELENCIA PARA EL DESARROLLO DE LA SOCIEDAD.</ns1:description>
    <ns1:isPlanedByMeansOf rdf:resource="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#mission"/>
    <ns1:supports rdf:resource="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#goal1"/>
    <ns1:supports rdf:resource="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#goal2"/>
    <ns1:supports rdf:resource="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#goal3"/>
    <ns1:supports rdf:resource="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#goal4"/>
    <ns1:supports rdf:resource="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#goal5"/>
  </ns1:Strategy>

  <ns1:Goal rdf:about="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#goal1">
    <ns1:elementOf rdf:resource="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#plan2015"/>
    <ns1:identifier>goa1</ns1:identifier>
    <ns1:title>Goal 1</ns1:title>
    <ns1:description>Implementar sistema de seguimiento y monitoreo de los programas de pregrado renovados, considerando las normativas institucionales, planes de capacitación pedagógica para docentes y uso de plataformas y TICS.</ns1:description>
    <ns1:amplifies rdf:resource="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#vision"/>
  </ns1:Goal>

  <ns1:Objective rdf:about="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#objective1">
    <ns1:elementOf rdf:resource="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#plan2015"/>
    <ns1:identifier>obj1</ns1:identifier>
    <ns1:title>Objective 1</ns1:title>
    <ns1:description>Convertir los programas de asignatura impartidas por el DCCTI al nuevo modelo educativo de la UBB - 100% de las asignaturas renovadas.</ns1:description>
    <ns1:measure>(N° de asignaturas renovadas / N° de asignaturas que solicitan renovación al DCCTI) * 100.</ns1:measure>
    <ns1:timetargeted>04-05-2015 al 31-12-2019</ns1:timetargeted>
    <ns1:quantifies rdf:resource="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#goal1"/>
  </ns1:Objective>

  <ns1:Tactic rdf:about="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#tactic2">
    <ns1:elementOf rdf:resource="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#plan2015"/>
    <ns1:identifier>tac2</ns1:identifier>
    <ns1:title>Tactic 2</ns1:title>
    <ns1:validity>does not record validity</ns1:validity>
    <ns1:description>Fortalecer el proceso de autoevaluación permanente para lograr y mantener la acreditación de la totalidad de los programas de pregrado y postgrado acreditables.</ns1:description>
    <ns1:implements rdf:resource="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#strategy1"/>
    <ns1:achieves>
      <ns1:Objective rdf:about="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#objective4">
        <ns1:elementOf rdf:resource="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#plan2015"/>
        <ns1:identifier>obj4</ns1:identifier>
        <ns1:title>Objective 4</ns1:title>
        <ns1:description>Participar de reuniones por área de especialidad, para analizar y promover mejoras en planes de estudio y metodologías empleadas el en aula - 100% de asistentes.</ns1:description>
        <ns1:measure>(N° de asistentes / N° de invitados)*100.</ns1:measure>
        <ns1:timetargeted>04-05-2015 al 31-12-2019</ns1:timetargeted>
        <ns1:quantifies rdf:resource="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#goal2"/>
      </ns1:Objective>
    </ns1:achieves>

  </ns1:Tactic>

  <ns1:Goal rdf:about="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#goal2">
    <ns1:elementOf rdf:resource="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#plan2015"/>
    <ns1:identifier>goa2</ns1:identifier>
    <ns1:title>Goal 2</ns1:title>
    <ns1:description>Potenciar los procesos de autoevaluación de pre y post grado a través de la formalización de comités internos y externos, de manera de mejorar los resultados de Acreditación.</ns1:description>
    <ns1:amplifies rdf:resource="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#vision"/>
  </ns1:Goal>

  <ns1:Tactic rdf:about="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#tactic3">
    <ns1:elementOf rdf:resource="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#plan2015"/>
    <ns1:identifier>tac3</ns1:identifier>
    <ns1:title>Tactic 3</ns1:title>
    <ns1:validity>does not record validity</ns1:validity>
    <ns1:description>Establecer políticas y gestionar recursos para mejorar la infraestructura y equipamiento para el desarrollo de las actividades académicas, docentes y extra programáticas de los estudiantes de la Universidad del Bío-Bío.</ns1:description>
    <ns1:implements rdf:resource="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#strategy1"/>
    <ns1:achieves>
      <ns1:Objective rdf:about="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#objective5">
        <ns1:elementOf rdf:resource="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#plan2015"/>
        <ns1:identifier>obj5</ns1:identifier>
        <ns1:title>Objective 5</ns1:title>
        <ns1:description>Elaborar un Plan de adquisición anual de equipamiento e insumos básicos - Plan elaborado y ejecutado al 100%</ns1:description>
        <ns1:measure>Plan elaborado</ns1:measure>
        <ns1:timetargeted>04-05-2015 al 31-12-2019</ns1:timetargeted>
        <ns1:quantifies rdf:resource="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#goal3"/>
      </ns1:Objective>
    </ns1:achieves>

    <ns1:achieves>
      <ns1:Objective rdf:about="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#objective6">
        <ns1:elementOf rdf:resource="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#plan2015"/>
        <ns1:identifier>obj6</ns1:identifier>
        <ns1:title>Objective 6</ns1:title>
        <ns1:description>Participar en la elaboración de un Plan de mejora anual de equipamiento por asignatura - Plan elaborado y ejecutado al 100% </ns1:description>
        <ns1:measure>plan formulado</ns1:measure>
        <ns1:timetargeted>04-05-2015 al 31-12-2019</ns1:timetargeted>
        <ns1:quantifies rdf:resource="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#goal3"/>
      </ns1:Objective>
    </ns1:achieves>

  </ns1:Tactic>

  <ns1:Goal rdf:about="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#goal3">
    <ns1:elementOf rdf:resource="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#plan2015"/>
    <ns1:identifier>goa3</ns1:identifier>
    <ns1:title>Goal 3</ns1:title>
    <ns1:description>Mejorar la infraestructura y equipamiento para el desarrollo de las actividades académicas de pre y post grado.</ns1:description>
    <ns1:amplifies rdf:resource="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#vision"/>
  </ns1:Goal>

  <ns1:Tactic rdf:about="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#tactic4">
    <ns1:elementOf rdf:resource="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#plan2015"/>
    <ns1:identifier>tac4</ns1:identifier>
    <ns1:title>Tactic 4</ns1:title>
    <ns1:validity>does not record validity</ns1:validity>
    <ns1:description>Fortalecer los procesos de articulación vertical, con la enseñanza media, tanto científico-humanista como técnico profesional, los programas de inserción y adaptación a la vida universitaria con la finalidad de mejorar la admisión y posicionar la oferta académica de la Universidad del Bío-Bío.</ns1:description>
    <ns1:implements rdf:resource="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#strategy1"/>
    <ns1:achieves>
      <ns1:Objective rdf:about="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#objective7">
        <ns1:elementOf rdf:resource="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#plan2015"/>
        <ns1:identifier>obj7</ns1:identifier>
        <ns1:title>Objective 7</ns1:title>
        <ns1:description>Definir y/o desarrollar actividades tendientes a la articulación vertical (talleres, capacitaciones, otras). - 100% de actividades planificadas realizadas</ns1:description>
        <ns1:measure>(N° actividades realizadas / N° actividades planificadas)*100</ns1:measure>
        <ns1:timetargeted>04-05-2015 al 31-12-2019</ns1:timetargeted>
        <ns1:quantifies rdf:resource="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#goal4"/>
      </ns1:Objective>
    </ns1:achieves>

  </ns1:Tactic>

  <ns1:Goal rdf:about="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#goal4">
    <ns1:elementOf rdf:resource="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#plan2015"/>
    <ns1:identifier>goa4</ns1:identifier>
    <ns1:title>Goal 4</ns1:title>
    <ns1:description>Fomentar la integración vertical con liceos, científicos humanistas, técnicos profesionales y CFT.</ns1:description>
    <ns1:amplifies rdf:resource="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#vision"/>
  </ns1:Goal>

  <ns1:Tactic rdf:about="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#tactic5">
    <ns1:elementOf rdf:resource="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#plan2015"/>
    <ns1:identifier>tac5</ns1:identifier>
    <ns1:title>Tactic 5</ns1:title>
    <ns1:validity>does not record validity</ns1:validity>
    <ns1:description>Potenciar las capacidades y competencias docentes de la dotación académica de la Universidad del Bío-Bío, para mejorar el desempeño en el aula y favorecer el desarrollo de la investigación en docencia.</ns1:description>
    <ns1:implements rdf:resource="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#strategy1"/>
    <ns1:achieves>
      <ns1:Objective rdf:about="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#objective8">
        <ns1:elementOf rdf:resource="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#plan2015"/>
        <ns1:identifier>obj8</ns1:identifier>
        <ns1:title>Objective 8</ns1:title>
        <ns1:description>Incentivar la participación en proyectos de investigación en docencia de pregrado - 20%</ns1:description>
        <ns1:measure>(N° de docentes que postulan a proyectos internos (FDD o FIDIE) / N° de docentes del DCCTI)*100</ns1:measure>
        <ns1:timetargeted>04-05-2015 al 31-12-2019</ns1:timetargeted>
        <ns1:quantifies rdf:resource="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#goal5"/>
      </ns1:Objective>
    </ns1:achieves>

    <ns1:achieves>
      <ns1:Objective rdf:about="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#objective9">
        <ns1:elementOf rdf:resource="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#plan2015"/>
        <ns1:identifier>obj9</ns1:identifier>
        <ns1:title>Objective 9</ns1:title>
        <ns1:description>Incentivar la participación de docentes en programas de capacitación ligados a las metodologías de enseñanza-aprendizaje en el aula y herramientas de gestión institucional - 100% </ns1:description>
        <ns1:measure>(N° de docentes del DCCTI que han participado en cursos de capacitación docente/ N° de docentes del DCCTI.)*100</ns1:measure>
        <ns1:timetargeted>04-05-2015 al 31-12-2019</ns1:timetargeted>
        <ns1:quantifies rdf:resource="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#goal5"/>
      </ns1:Objective>
    </ns1:achieves>

  </ns1:Tactic>

  <ns1:Goal rdf:about="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#goal5">
    <ns1:elementOf rdf:resource="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#plan2015"/>
    <ns1:identifier>goa5</ns1:identifier>
    <ns1:title>Goal 5</ns1:title>
    <ns1:description>Fomentar la investigación en docencia e incentivar la participación de académicos en programas de capacitación docente.</ns1:description>
    <ns1:amplifies rdf:resource="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#vision"/>
  </ns1:Goal>

  <ns1:Strategy rdf:about="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#strategy2">
    <ns1:elementOf rdf:resource="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#plan2015"/>
    <ns1:identifier>str2</ns1:identifier>
    <ns1:title>Strategy 2</ns1:title>
    <ns1:description>INVESTIGACIÓN, DESARROLLO E INNOVACIÓN APORTANDO CONOCIMIENTO AVANZADO AL ENTORNO.</ns1:description>
    <ns1:isPlanedByMeansOf rdf:resource="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#mission"/>
    <ns1:supports rdf:resource="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#goal6"/>
    <ns1:supports rdf:resource="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#goal7"/>
    <ns1:supports>
      <ns1:Goal rdf:about="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#goal8">
        <ns1:elementOf rdf:resource="http://parra.chillan.ubiobio.cl:8070/clamunoz/planes/dccti/dccti#plan2015"/>
